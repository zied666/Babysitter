{% extends ':Back:base.html.twig' %}
{% block title %}Paypal{% endblock %}
{% block topbody %}
    {{ parent() }}
    <li><a href="{{ path('back_administration_paypal') }}">Paypal</a></li>
{% endblock %}
{% block body %}
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    {{ form_start(form) }}
                    {{ form_errors(form) }}
                    <div class="row">
                        {{ formulaire.input("col-md-3","Account",form.account) }}
                        {{ formulaire.input("col-md-3","",form.testMode,false) }}
                    </div>
                    {{ formulaire.submit("Submit") }}
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </div>
{% endblock %}